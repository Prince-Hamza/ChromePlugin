{
    "manifest_version": 2,
    "name": "Chrome Plugin",
    "description": "Testing Plugin",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "Jquery.js",
                "Axios.js" ,
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "icon.jpg",
        "default_popup": "popup.html"
    },
    
    "permissions": [
        "activeTab",
        "storage",
        "https://ajax.googleapis.com/",
        "https://www.popshop.live/"
    ]
        
}